selectAll=SELECT BNO, TITLE, CONTENT, CREATE_DATE, WRITER FROM BOARD WHERE DELETE_YN = 'N'
selectOne=SELECT BNO, TITLE, CONTENT, CREATE_DATE, WRITER FROM BOARD WHERE DELETE_YN = 'N' AND BNO = ?
insertBoard=INSERT INTO BOARD VALUES(SEQ_BOARD.NEXTVAL, ?, ?, DEFAULT, ?, DEFAULT)
updateBoard1=UPDATE BOARD SET TITLE = ? WHERE DELETE_YN = 'N' AND BNO = ?
updateBoard2=UPDATE BOARD SET CONTENT = ? WHERE DELETE_YN = 'N' AND BNO = ?
deleteBoard=UPDATE BOARD SET DELETE_YN = 'Y' WHERE BNO = ?
checkBoard=SELECT COUNT(*) FROM BOARD WHERE DELETE_YN = 'N' AND BNO = ? AND WRITER = ?