package com.kh.example.run;

import com.kh.example.dto.Task;

public class Run {

	public static void main(String[] args) {
		Task task = new Task();
		Task task1 = new Task();
		Thread t1 = new Thread(task);
		Thread t2 = new Thread(task1);

		t1.setName("t1-Thread");
		t2.setName("t2-Thread");

		t1.start();
		t2.start();
	}

}